<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <solution>
    <add key="disableSourceControlIntegration" value="true" />
  </solution>
  <packageSources>
    <clear />
    <add key="dotnet-public" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet-public/nuget/v3/index.json" />
    <add key="dotnet-tools" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet-tools/nuget/v3/index.json" />
    <add key="dotnet-eng" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet-eng/nuget/v3/index.json" />
    <add key="vs-buildservices" value="https://pkgs.dev.azure.com/azure-public/vside/_packaging/vs-buildservices/nuget/v3/index.json" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="dotnet-eng">
      <reservedPrefix pattern="*" />
      <package pattern="MicroBuild.*" />
      <package pattern="Microsoft.*" />
      <package pattern="sn" />
      <package pattern="Wix" />
      <package pattern="xunit" />
      <package pattern="xunit.*" />
    </packageSource>
    <packageSource key="dotnet-public">
      <reservedPrefix pattern="Microsoft.MyPackage" />
      <package pattern="*" />
      <package pattern="MicroBuild.Core" />
      <package pattern="Microsoft.*" />
      <package pattern="NuGet.*" />
      <package pattern="Wix" />
      <package pattern="xunit.*" />
    </packageSource>
    <packageSource key="dotnet-tools">
      <reservedPrefix pattern="*" />
      <package pattern="Microsoft.*" />
      <package pattern="NuGet.*" />
    </packageSource>
    <packageSource key="vs-buildservices">
      <reservedPrefix pattern="*" />
      <package pattern="Microsoft.*" />
    </packageSource>
  </packageSourceMapping>
  <disabledPackageSources>
    <clear />
  </disabledPackageSources>
</configuration>
